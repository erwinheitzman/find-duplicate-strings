{
	"name": "find-duplicate-strings",
	"version": "3.0.0",
	"description": "Easy to use CLI that finds duplicate strings in a directory and stores them in a external file for easy reference",
	"author": "Erwin Heitzman",
	"homepage": "https://github.com/erwinheitzman/find-duplicate-strings/blob/master/README.md",
	"license": "MIT",
	"main": "lib/cli/index.js",
	"types": "lib/cli/index.d.ts",
	"bin": "lib/cli/index.js",
	"type": "module",
	"files": ["/lib"],
	"scripts": {
		"prepare": "husky || true",
		"prepublishOnly": "tsc",
		"test": "node --experimental-vm-modules node_modules/.bin/jest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/erwinheitzman/find-duplicate-strings.git"
	},
	"keywords": [
		"duplicate",
		"duplicates",
		"strings",
		"duplication",
		"scan",
		"find",
		"find-duplicate-strings",
		"search"
	],
	"bugs": {
		"url": "https://github.com/erwinheitzman/find-duplicate-strings/issues"
	},
	"dependencies": {
		"@inquirer/prompts": "^7.4.1",
		"@types/node": "^22.14.0",
		"commander": "^13.1.0",
		"glob": "^11.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@eslint/js": "^9.24.0",
		"@jest/globals": "^29.7.0",
		"@types/jest": "^29.5.13",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.1",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"lint-staged": "^16.1.0",
		"prettier": "^3.5.3",
		"ts-jest": "^29.3.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.29.1"
	},
	"lint-staged": {
		"*": ["biome check --write --no-errors-on-unmatched"]
	}
}
